{
  "expo": {
    "name": "Foodiez",
    "slug": "Foodiez",
    "description": "Foodiez is a food delivery app that allows you to order food from your favorite restaurants and have it delivered to your door.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/foodiez-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "assets/foodiez-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#200763"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}